{
  "name": "ng2-auto-complete",
  "version": "0.5.5",
  "description": "Angular2 Input Autocomplete",
  "license": "MIT",
  "main": "release/ng2-auto-complete.umd.js",
  "typings": "release/ng2-auto-complete.d.ts",
  "scripts": {
    "check": "npm-check --skip-unused",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "start": "http-server -o -p 9001",
    "build": "./build.sh",
    "release": "npm-run-all -s clean:* release:ts -p release:replace release:build:* -s release:minify",
    "release:ts": "tsc --outDir release",
    "release:replace": "node tasks/replace.js",
    "release:build:umd": "rollup -c -f umd -o release/ng2-auto-complete.umd.js",
    "release:build:cjs": "rollup -c -f cjs -o release/ng2-auto-complete.cjs.js",
    "release:build:es": "rollup -c -f es -o release/ng2-auto-complete.es.js",
    "release:minify": "uglifyjs release/ng2-auto-complete.js --source-map release/ng2-auto-complete.min.js.map --source-map-url release/ng2-auto-complete.js.map --compress --mangle --screw-ie8 --output release/ng2-auto-complete.min.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "http-server": "^0.9.0",
    "ng2-inline-template": "^0.1.1",
    "npm-check": "^5.2.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.3",
    "rollup": "^0.34.13",
    "rollup-plugin-sourcemaps": "^0.3.5",
    "rollup-plugin-typescript": "^0.8.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2-ui/ng2-auto-complete.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/ng2-ui/ng2-auto-complete/issues"
  },
  "homepage": "https://github.com/ng2-ui/ng2-auto-complete#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "angular",
    "auto-complete",
    "input",
    "select"
  ]
}
